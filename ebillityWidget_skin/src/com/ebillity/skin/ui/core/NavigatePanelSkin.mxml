<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:mx="library://ns.adobe.com/flex/mx"
		xmlns:controls="com.ebillity.core.view.ui.controls.*" xmlns:controls1="com.ebillity.skin.ui.controls.*">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("com.ebillity.core.view.ui.core.NavigatePanel")]
	</fx:Metadata>
	<fx:Script>
		<![CDATA[
			import com.ebillity.core.constants.AssetConstants;
		]]>
	</fx:Script>
	<s:Rect width="100%" height="100%">
		<s:fill>
			<s:SolidColor color="#e7e8e9"/>
		</s:fill>
	</s:Rect>

	<s:HGroup width="100%"
			  height="100%" paddingLeft="5" paddingRight="5" paddingTop="5" paddingBottom="5">

		<s:Button id="recycleBin"
							  width="30"
							  height="30"
							  skinClass="com.ebillity.skin.ui.controls.Buttons.recicleBinButton"
							  icon="{AssetConstants.RECYCLE_BIN_ICON}"/>

		<controls:ColorButton id="button1"
							  height="30"
							  visible="false"
							  includeInLayout="false"/>

		<s:Line width="100%"/>

		<controls:ColorButton id="button2"
							  fontSize="14"
							  height="30"
							  width="120"
							  visible="false"
							  includeInLayout="false"/>

		<controls:ColorButton id="button3"
							  height="30"
							  width="120"
							  fontSize="14"
							  buttonColor="#51AA47"
							  
							  visible="false"
							  includeInLayout="false"/>
	</s:HGroup>
</s:Skin>
